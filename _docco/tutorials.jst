<div class="row-fluid">
  <div class="container documentation-main-container">
    <div class="content span8">
      <% for (var i = 0, l = sections.length; i<l; i++) { %>
        <% var section = sections[i]; %>
        <%= section.docsHtml %>
        <% if (!(/^\s*$/).test(section.codeText)) { %>
          <%= section.codeHtml %>
        <% } %>
      <% } %>
    </div>
  </div>
</div>
